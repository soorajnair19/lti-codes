package com.hibernate.inheritance;

import javax.persistence.Entity;

@Entity
public class Task {
private String tname;

public Task() {
	super();
	// TODO Auto-generated constructor stub
}

public Task(String tname) {
	super();
	this.tname = tname;
}

public String getTname() {
	return tname;
}

public void setTname(String tname) {
	this.tname = tname;
}


}
